<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.xwiki.platform</groupId>
    <artifactId>xwiki-platform-core</artifactId>
    <version>3.5.1</version>
  </parent>
  <groupId>org.xwiki.contrib</groupId>
  <artifactId>xwiki-crash</artifactId>
  <name>XWiki Contrib - CRaSH integration</name>
  <version>1.1-SNAPSHOT</version>
  <packaging>pom</packaging>
  <description>CRaSH integration in XWiki.</description>
  <scm>
    <connection>scm:git:git://github.com/jvelo/xwiki-crash.git</connection>
    <developerConnection>scm:git:git@github.com:jvelo/xwiki-crash.git</developerConnection>
    <url>https://github.com/jvelo/xwiki-crash</url>
  </scm>
  <distributionManagement>
    <repository>
      <id>xwiki-staging</id>
      <name>XWiki Staging Repository</name>
      <url>http://nexus.xwiki.org/nexus/service/local/staging/deploy/maven2/</url>
    </repository>
  </distributionManagement>
  <properties>
    <xwiki.version>3.5.1</xwiki.version>
  </properties>
  <modules>
    <module>xwiki-crash-api</module>
    <module>xwiki-crash-ui</module>
  </modules>
</project>
